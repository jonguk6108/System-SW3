ADDR	addr_decoder.h	/^typedef uint64_t ADDR;$/;"	t
ADDR_BITS	addr_decoder.h	6;"	d
ADDR_INFO	addr_decoder.h	/^}ADDR_INFO;$/;"	t	typeref:struct:addr_info
BYTE	addr_decoder.h	7;"	d
CC	Makefile	/^CC = g++$/;"	m
CXXFLAGS	Makefile	/^CXXFLAGS = -g -O2$/;"	m
OBJS	Makefile	/^OBJS = addr_decoder.o main.o$/;"	m
OFFSET	addr_decoder.h	/^typedef uint32_t OFFSET;$/;"	t
PAGE_SIZE	addr_decoder.h	5;"	d
STUDENT_ID	main.cc	7;"	d	file:
TARGET	Makefile	/^TARGET = ..\/bin\/addr_decoder.out$/;"	m
addr_info	addr_decoder.h	/^typedef struct addr_info {$/;"	s
get_addr	addr_decoder.cc	/^ADDR get_addr (ADDR_INFO info){ return info.virtual_page_number * PAGE_SIZE + (ADDR)info.offset; }$/;"	f
get_addr_info	addr_decoder.cc	/^ADDR_INFO get_addr_info (ADDR input_addr){$/;"	f
main	main.cc	/^int main (int argc, char *argv[]) {$/;"	f
offset	addr_decoder.h	/^    OFFSET offset;$/;"	m	struct:addr_info
print_addr_info	addr_decoder.cc	/^void print_addr_info (ADDR addr){$/;"	f
print_addr_info	addr_decoder.cc	/^void print_addr_info (ADDR_INFO info) {$/;"	f
virtual_page_number	addr_decoder.h	/^    ADDR virtual_page_number;$/;"	m	struct:addr_info
